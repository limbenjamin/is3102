<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <navigation-case>
            <from-outcome>dash</from-outcome>
            <to-view-id>/dash.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>modifyparticulars</from-outcome>
            <to-view-id>/common/modifyparticulars.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>messaging</from-outcome>
            <to-view-id>/common/messaging.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>messaging2</from-outcome>
            <to-view-id>/common/messaging2.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>broadcast</from-outcome>
            <to-view-id>/common/broadcast.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>changepassword</from-outcome>
            <to-view-id>/common/changepassword.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
